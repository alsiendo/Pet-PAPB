        <activity android:name=".UI.PCMalangTwoDetailActivity" />
        <activity android:name=".UI.PCMalangOneDetailActivity" />
        <activity android:name=".UI.PCSurabayaActivity" />
        <activity android:name=".UI.PCMalangActivity" />
