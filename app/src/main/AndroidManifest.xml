<!--        HomePage 2-->
        <activity android:name=".UI.Homepage2.Homepage2Activity" />
        <activity
            android:name=".UI.Adoption2.PostPetActivity"
            android:parentActivityName=".UI.Homepage2.Homepage2Activity"/>
        <activity android:name=".UI.Homepage2.Adoption2Activity"
