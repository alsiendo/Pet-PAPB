        <activity android:name=".UI.DummyActivity" />

        <activity android:name=".UI.Homepage1.HomepageActivity" /> <!-- <activity android:name=".UI.Adoption1.AdoptionActivity"> -->

<!--        Login-->
        <activity android:name=".UI.Login.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UI.Login.SignupActivity"
            android:parentActivityName=".UI.Login.StartActivity" />
        <activity
            android:name=".UI.Login.LoginActivity"
            android:parentActivityName=".UI.Login.StartActivity" />

<!--        Adoption1-->
        <activity android:name=".UI.Adoption1.AdoptionActivity"
            android:parentActivityName=".UI.Homepage1.HomepageActivity"/>
        <activity
            android:name=".UI.Adoption1.DetailAdoptionActivity"
