<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pet_papb">
    
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PetPAPB">
        <activity android:name=".UI.MessageActivity" >
        </activity>
        <activity android:name=".UI.Kosongan2Activity" />
        <activity android:name=".UI.KosonganActivity" />

        <activity android:name=".UI.PCSurabayaTwoDetailActivity" />
